// Place your key bindings in this file to overwrite the defaults
[
{ "key": "ctrl+j s",                 "command": "workbench.action.quickOpen" },
{ "key": "ctrl+j z",               "command": "workbench.action.toggleZenMode" },
{ "key": "ctrl+j j",                 "command": "workbench.action.toggleSidebarVisibility" },
{ "key": "ctrl+j e",           "command": "workbench.view.explorer" },
{ "key": "ctrl+j g",             "command": "editor.action.startFindReplaceAction" },
{ "key": "cmd+j",          "command": "editor.action.insertCursorBelow",
                                     "when": "editorTextFocus" },
{ "key": "ctrl+j l",           "command": "workbench.action.showAllEditors" },
{ "key": "ctrl+[BracketRight]",                   "command": "editor.action.goToDeclaration",
                                     "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
{ "key": "ctrl+c",                "command": "editor.action.webvieweditor.hideFind",
                                     "when": "webviewEditorFocus && webviewFindWidgetVisible" },
{ "key": "ctrl+c",                "command": "settings.action.clearSearchResults",
                                     "when": "inSettingsSearch" },
{ "key": "ctrl+c",                "command": "closeFindWidget",
                                     "when": "editorFocus && findWidgetVisible" },
{ "key": "ctrl+c",                "command": "closeBreakpointWidget",
                                     "when": "breakpointWidgetVisible && editorFocus" },
{ "key": "ctrl+c",                "command": "leaveEditorMessage",
                                     "when": "messageVisible" },
{ "key": "ctrl+c",                "command": "leaveSnippet",
                                     "when": "editorTextFocus && inSnippetMode" },
{ "key": "ctrl+c",                "command": "closeMarkersNavigation",
                                     "when": "editorFocus && markersNavigationVisible" },
{ "key": "ctrl+c",                "command": "closeParameterHints",
                                     "when": "editorTextFocus && parameterHintsVisible" },
{ "key": "ctrl+c",                "command": "hideSuggestWidget",
                                     "when": "editorTextFocus && suggestWidgetVisible" },
{ "key": "ctrl+c",                "command": "keybindings.editor.clearSearchResults",
                                     "when": "inKeybindings && inKeybindingsSearch" },
{ "key": "ctrl+c",                "command": "list.clear",
                                     "when": "listFocus && !inputFocus" },
{ "key": "ctrl+c",                "command": "search.action.cancel",
                                     "when": "listFocus && searchViewletVisible && !inputFocus" },
{ "key": "ctrl+c",                "command": "search.action.cancel",
                                     "when": "listFocus && searchViewletVisible && !inputFocus" },
{ "key": "ctrl+c",                "command": "workbench.action.closeQuickOpen",
                                     "when": "inQuickOpen" },
{ "key": "ctrl+c",                "command": "workbench.action.hideInterfaceOverview",
                                     "when": "interfaceOverviewVisible" },
{ "key": "ctrl+c",                "command": "workbench.action.terminal.hideFindWidget",
                                     "when": "terminalFindWidgetVisible && terminalFocus" },
{ "key": "ctrl+c",                "command": "closeReferenceSearch",
                                     "when": "referenceSearchVisible && !config.editor.stablePeek" },
{ "key": "ctrl+c",                "command": "notifications.hideList",
                                     "when": "notificationCenterVisible" },
{ "key": "ctrl+c",                "command": "closeReferenceSearchEditor",
                                     "when": "inReferenceSearchEditor && !config.editor.stablePeek" },
{ "key": "ctrl+c",                "command": "cancelSelection",
                                     "when": "editorHasSelection && editorTextFocus" },
{ "key": "ctrl+j",                "command": "showNextParameterHint",
                                     "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible" },
{ "key": "ctrl+k",                "command": "showPrevParameterHint",
                                     "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible" },
{ "key": "ctrl+j",                "command": "selectNextSuggestion",
                                     "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible" },
{ "key": "ctrl+k",                "command": "selectPrevSuggestion",
                                     "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible" },
{ "key": "ctrl+j",                "command": "workbench.action.quickOpenSelectNext",
                                     "when": "inQuickOpen" },
{ "key": "ctrl+k",                "command": "workbench.action.quickOpenSelectPrevious",
                                     "when": "inQuickOpen" },
]