// Place your key bindings in this file to overwrite the defaults
[
{ "key": "shift+/",                 "command": "actions.find" },
{ "key": "ctrl+j s",                 "command": "workbench.action.quickOpen" },
{ "key": "ctrl+j z",               "command": "workbench.action.toggleZenMode" },
{ "key": "ctrl+j j",                 "command": "workbench.action.toggleSidebarVisibility" },
{ "key": "ctrl+j e",           "command": "workbench.view.explorer" },
{ "key": "ctrl+j g",           "command": "workbench.action.findInFiles" },
{ "key": "cmd+j",          "command": "editor.action.insertCursorBelow",
                                     "when": "editorTextFocus" },
{ "key": "ctrl+j l",           "command": "workbench.action.showAllEditors" },
{ "key": "ctrl+[BracketRight]",                   "command": "editor.action.goToDeclaration",
                                     "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
{ "key": "ctrl+j",                "command": "showNextParameterHint",
                                     "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible" },
{ "key": "ctrl+k",                "command": "showPrevParameterHint",
                                     "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible" },
{ "key": "ctrl+j",                "command": "selectNextSuggestion",
                                     "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible" },
{ "key": "ctrl+k",                "command": "selectPrevSuggestion",
                                     "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible" },
{ "key": "ctrl+j",                "command": "workbench.action.quickOpenSelectNext",
                                     "when": "inQuickOpen" },
{ "key": "ctrl+k",                "command": "workbench.action.quickOpenSelectPrevious",
                                     "when": "inQuickOpen" },
]
