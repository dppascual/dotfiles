// Place your key bindings in this file to overwrite the defaults
[
{ "key": "ctrl+j s",                 "command": "workbench.action.quickOpen" },
{ "key": "ctrl+j z",               "command": "workbench.action.toggleZenMode" },
{ "key": "ctrl+j d",                 "command": "workbench.action.toggleSidebarVisibility" },
{ "key": "ctrl+j e",           "command": "workbench.view.explorer" },
{ "key": "ctrl+j g",          "command": "workbench.view.scm" },
{ "key": "ctrl+n",                 "command": "extension.vim_cmd+d",
                                     "when": "editorTextFocus && vim.active && vim.use<D-d> && !inDebugRepl" },
{ "key": "alt+cmd+down",          "command": "editor.action.insertCursorBelow",
                                     "when": "editorTextFocus" },
]